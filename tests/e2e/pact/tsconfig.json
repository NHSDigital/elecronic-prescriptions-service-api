{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es6",
    "lib": [
      "es2019"
    ],
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {
      "@models": ["../../../models"],
      "@coordinator": ["../../../coordinator"]
    }
  },
  "include": [
    "specs/**/*",
    ".",
    "../../../../../coordinator"
  ],
  "references": [
    {
      "path": "../../../models"
    },
    {
      "path": "../../../coordinator"
    }
  ]
}
