{
  "name": "apim-e2e-tests",
  "version": "0.0.1",
  "description": "E2E Pact tests for Electronic Prescription Service API",
  "scripts": {
    "test": "jest",
    "lint": "eslint . --ext .ts --max-warnings 0 --fix",
    "check-licenses": "node_modules/.bin/license-checker --failOn GPL --failOn LGPL",
    "generate-postman-collection": "ts-node postman-pact.ts --use_strict",
    "clear-cache": "jest --clearCache",
    "create-sandbox-pacts": "jest sandbox",
    "create-live-pacts": "jest live",
    "publish-pacts": "ts-node ./broker/publish.ts --use_strict",
    "verify-pacts": "ts-node ./broker/verify.ts --use_strict",
    "generate-token": "ts-node ./services/get-access-token.ts"
  },
  "author": "NHS Digital",
  "license": "MIT",
  "config": {
    "pact_do_not_track": true
  },
  "devDependencies": {
    "@pact-foundation/pact": "10.0.0-beta.22",
    "@types/axios": "^0.14.0",
    "@types/crypto-js": "^4.0.1",
    "@types/jest": "24.9.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lossless-json": "1.0.0",
    "@types/node": "14.0.13",
    "@types/supertest": "2.0.9",
    "@typescript-eslint/eslint-plugin": "3.2.0",
    "@typescript-eslint/parser": "3.2.0",
    "axios": "^0.21.1",
    "eslint": "6.8.0",
    "jest": "24.9.0",
    "jest-pact": "0.6.0",
    "jsonwebtoken": "^8.5.1",
    "license-checker": "25.0.1",
    "lossless-json": "1.0.4",
    "supertest": "4.0.2",
    "ts-jest": "26.1.0",
    "ts-node": "8.10.2",
    "typescript": "3.9.5",
    "uuid": "8.3.0",
    "xml-js": "1.6.11"
  },
  "dependencies": {
    "crypto-js": "^4.0.0"
  }
}
