{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": false,
    "moduleResolution": "node",
    "module": "es6",
    "target": "es5",
    "lib": [
      "es2019",
      "dom"
    ],
    "baseUrl": ".",
    "paths": {
      "@models": ["../../models", "./models"]
    }
  },
  "skipLibCheck": true,
  "files": ["client/index.ts", "client/data/prescriptions.ts"],
  "references": [
    {
      "path": "../../models"
    }
  ]
}
