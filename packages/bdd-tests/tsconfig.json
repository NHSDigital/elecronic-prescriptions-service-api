{
  "compilerOptions": {
    "target": "ES2021",
    "module": "commonjs",
    "resolveJsonModule": true,
    "downlevelIteration": true,
    "strict": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["**/*.ts"],
  "exclude": ["node_modules"]
}
